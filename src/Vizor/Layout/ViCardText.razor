@inherits ViComponentBase

<p class="card-text @CssClass" @attributes="AdditionalAttributes">
	@ChildContent
	@Text
</p>

@code {

	// see https://getbootstrap.com/docs/5.2/components/card/

	[Parameter]
	public RenderFragment? ChildContent { get; set; }

	[Parameter]
	public string? Text { get; set; }
}
